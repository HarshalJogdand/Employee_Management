<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <ViewSettingsDialog
        id="settingsDialog"
        confirm="handleConfirm2"
        cancel="handleCancel"
        resetFilters="handleResetFilters">
        <filterItems>
            <ViewSettingsFilterItem text="Project Name" key="ID" items="{projetname>/}">
                <items>
                    <ViewSettingsItem text="{projetname>PROJECT_NAME}" key="{projetname>PROJECT_NAME}" />
                        </items>
                </ViewSettingsFilterItem>       
        </filterItems>
    </ViewSettingsDialog>
</core:FragmentDefinition>